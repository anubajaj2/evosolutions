{
  "name": "Inquiry",
  "plural": "Inquries",
  "base": "PersistedModel",
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "EmailId": {
      "type": "string",
      "required": true
    },
    "FatherName": {
      "type": "string",
      "default": null
    },
    "MotherName": {
      "type": "string",
      "default": null
    },
    "Date": {
      "type": "date"
    },
    "City": {
      "type": "string",
      "default": "Gurgaon"
    },
    "Address":{
      "type": "string",
      "default":null
    },
    "Phone": {
      "type": "number",
      "id": true,
      "default":null
    },
    "EmergencyContactName": {
      "type": "string",
      "default": null
    },
    "EmergencyContactNo": {
      "type": "string",
      "default": null
    },
    "SoftDelete": {
      "type": "boolean",
      "default": false
    },
    "Remarks": {
      "type": "string",
      "default": null
    },
    "HearAbout":{
      "type": "string",
      "default": null
    },
    "CreatedOn": {
      "type": "date"
    },
    "CreatedBy": {
      "type": "string",
      "default": null
    },
    "ChangedOn": {
      "type": "date"
    },
    "ChangedBy": {
      "type": "string",
      "default": null
    }
  },
  "validations": [],
  "relations": {
    "CreatedByTo": {
			"type": "belongsTo",
			"model": "AppUser",
			"foreignKey": "CreatedBy"
		},
    "ChangedByTo": {
			"type": "belongsTo",
			"model": "AppUser",
			"foreignKey": "ChangedBy"
		},
    "ToWard":{
      "type":"hasMany",
      "model":"Ward",
      "foreignKey":"InquiryId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
