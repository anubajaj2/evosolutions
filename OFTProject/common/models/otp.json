{
	"name": "Otp",
	"plural": "Otps",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"properties": {
		"Number": {
			"type": "string",
			"default": "null",
			"required": true
		},
		"OTP": {
			"type": "string",
			"required": true,
			"default": "null"
		},
		"CreatedOn": {
			"type": "date",
			"default": "null"
		},
		"Attempts":{
			"type": "number",
			"default": "null",
			"required": false
		}
	},
	"scope": {
		"order": "CreatedOn DESC"
	},
	"validations": [],
  	"relations": {
		"Student": {
			"type": "belongsTo",
			"model": "Student",
			"foreignKey": "StudentId"
		},
	   "ServerPay":{
			"type": "hasOne",
			"model": "ServerPay",
			"foreignKey": "ServerId"
	   }
	},
	"acls": [{
		"accessType": "*",
		"principalType": "ROLE",
		"principalId": "$unauthenticated",
		"permission": "DENY"
	}],
	"methods": {}
}
