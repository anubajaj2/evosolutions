{
  "name": "HolidayCalendar",
  "plural": "HolidayCalendars",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Day": {
      "type": "string",
      "required": true,
      "default": "null"
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Occasion": {
      "type": "string",
      "required": true,
      "default": "null"      
    },
    "CreatedOn": {
      "type": "date"
    },
    "CreatedBy": {
      "type": "string",
      "default": "null"
    },
    "ChangedOn": {
      "type": "date"
    },
    "ChangedBy": {
      "type": "string",
      "default": "null"
    }
  },
  "validations": [],
  "relations": {
    "ToCourse": {
			"type": "belongsTo",
			"model": "CoursesMst",
			"foreignKey": "CourseId"
		},
    "ToChangedBy": {
			"type": "belongsTo",
			"model": "AppUser",
			"foreignKey": "ChangedBy"
		},
    "ToCreatedBy": {
			"type": "belongsTo",
			"model": "AppUser",
			"foreignKey": "CreatedBy"
		},
    "ToTrainer":{
      "type":"belongsTo",
      "model":"Trainer",
      "foreignKey":"TrainerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
